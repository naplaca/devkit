{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "module": "esnext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "plugins": [{ "name": "next" }],
    "moduleResolution": "bundler",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true
  }
}
